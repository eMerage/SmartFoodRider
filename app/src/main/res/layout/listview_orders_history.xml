<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp">


    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_cardview"
        card_view:cardCornerRadius="5dp">

        <RelativeLayout
            android:id="@+id/relativelayout_main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/textView_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/openSansLight8"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:freezesText="true"
                android:text=""
                android:textColor="@color/app_black"
                android:textSize="15sp" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/openSansLight12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/openSansLight8"
                android:layout_marginLeft="5dp"
                android:freezesText="true"
                android:textColor="@color/app_black"
                android:textSize="15sp"
                android:text="Time : " />


            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/textView_ordernumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginRight="5dp"
                android:freezesText="true"
                android:text=""
                android:textColor="@color/app_black"
                android:textSize="16sp" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/openSansLight8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:freezesText="true"
                android:text="Order Number :"
                android:textColor="@color/app_black"
                android:textSize="16sp" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/openSansLight2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView_time"
                android:textColor="@color/app_black"
                android:layout_marginLeft="5dp"
                android:textSize="15sp"
                android:text="Outlet :" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/textView_outlet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/textView_time"
                android:layout_marginRight="5dp"
                android:textColor="@color/app_black"
                android:textSize="15sp"
                android:text="" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/openSansLight4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/openSansLight2"
                android:textColor="@color/app_black"
                android:layout_marginLeft="5dp"
                android:textSize="15sp"
                android:text="Total Amount :" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/textView_amount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/openSansLight2"
                android:textColor="@color/app_black"
                android:layout_marginRight="5dp"
                android:textSize="15sp"
                android:text="" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/openSansLight6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/openSansLight4"
                android:textColor="@color/app_black"
                android:layout_marginLeft="5dp"
                android:textSize="15sp"
                android:text="Total Quantity :" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/textView_qty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/openSansLight4"
                android:textColor="@color/app_black"
                android:layout_marginRight="5dp"
                android:textSize="15sp"
                android:text="" />

            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/openSansLight15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/openSansLight6"
                android:textColor="@color/app_black"
                android:layout_marginLeft="5dp"
                android:textSize="15sp"
                android:text="Customer :" />


            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/textView_customer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/openSansLight6"
                android:layout_marginRight="5dp"
                android:textColor="@color/app_black"
                android:text=""
                android:textSize="15sp"
                android:layout_alignParentRight="true" />




            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/openSansLight159"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/openSansLight15"
                android:textColor="@color/app_black"
                android:layout_marginLeft="5dp"
                android:textSize="15sp"
                android:text="Customer No :" />


            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/textView_customer_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/textView_customer"
                android:layout_marginRight="5dp"
                android:textColor="@color/app_black"
                android:text=""
                android:textSize="15sp"
                android:layout_alignParentRight="true" />


            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/openSansLight155"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/openSansLight159"
                android:textColor="@color/app_black"
                android:layout_marginLeft="5dp"
                android:textSize="15sp"
                android:text="address :" />


            <emerge.project.onmealrider.ui.font.OpenSansLight
                android:id="@+id/textView_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/textView_customer_number"
                android:layout_marginRight="5dp"
                android:textColor="@color/app_black"
                android:text=""
                android:textSize="15sp"
                android:layout_alignParentRight="true" />


        </RelativeLayout>
    </android.support.v7.widget.CardView>

</RelativeLayout>







